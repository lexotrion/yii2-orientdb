{
    "name": "phantom-d/yii2-orientdb",
    "description": "OrientDb extension for the Yii framework",
    "type": "yii2-extension",
    "keywords": ["yii2", "orientdb", "active-record"],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Anton Ermolovich",
            "email": "anton.ermolovich@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.x-dev",
        "ostico/phporient": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.24",
        "monolog/monolog": "1.11.*",
        "phpdocumentor/template-zend": "1.3.0",
        "phpdocumentor/phpdocumentor": "dev-master",
        "satooshi/php-coveralls": "dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "phantomd\\orientdb\\": "src/"
        }
    }
}
